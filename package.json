{
  "name": "@qubit-ltd/common-decorator",
  "version": "3.8.11",
  "description": "A JavaScript ES7 library of common decorators",
  "author": "Haixing Hu (starfish.hu@gmail.com)",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Haixing-Hu/js-common-decorator.git"
  },
  "bugs": {
    "url": "https://github.com/Haixing-Hu/js-common-decorator/issues"
  },
  "homepage": "https://github.com/Haixing-Hu/js-common-decorator",
  "keywords": [
    "JavaScript",
    "decorator"
  ],
  "main": "dist/common-decorator.min.cjs",
  "module": "dist/common-decorator.min.mjs",
  "exports": {
    ".": {
      "require": "./dist/common-decorator.min.cjs",
      "import": "./dist/common-decorator.min.mjs"
    }
  },
  "files": [
    "dist/**/*",
    "doc/**/*",
    "LICENSE",
    "README.md",
    "README.zh_CN.md"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rvf dist doc",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.mjs",
    "build:dev": "cross-env NODE_ENV=development rollup -c rollup.config.mjs",
    "build:all": "npm run clean && npm run doc && npm run build:dev && npm run build",
    "test": "jest",
    "lint": "eslint ./src ./test",
    "doc": "jsdoc -c jsdoc.json",
    "es5": "check-es-version -e 5 -s true",
    "deploy": "npm run lint && npm run test && npm run build:all && npm publish --registry='https://registry.npmjs.com/' --access public"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "peerDependencies": {
    "@babel/runtime": "^7.26.0",
    "@qubit-ltd/clone": "^1.12.3",
    "@qubit-ltd/common-util": "^5.3.4",
    "@qubit-ltd/common-validation-rule": "^1.4.4",
    "@qubit-ltd/json": "^1.1.4",
    "@qubit-ltd/naming-style": "^1.3.4",
    "@qubit-ltd/type-detect": "^1.2.3",
    "@qubit-ltd/typeinfo": "^3.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@babel/eslint-parser": "^7.26.8",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.26.8",
    "@babel/preset-env": "^7.26.8",
    "@babel/runtime": "^7.26.7",
    "@jest/core": "^29.7.0",
    "@qubit-ltd/clone": "^1.12.3",
    "@qubit-ltd/common-util": "^5.3.4",
    "@qubit-ltd/common-validation-rule": "^1.4.4",
    "@qubit-ltd/eslint-config": "^1.3.4",
    "@qubit-ltd/jsdoc-minami": "^1.5.2",
    "@qubit-ltd/json": "^1.1.4",
    "@qubit-ltd/naming-style": "^1.3.4",
    "@qubit-ltd/rollup-builder": "^1.8.9",
    "@qubit-ltd/type-detect": "^1.2.3",
    "@qubit-ltd/typeinfo": "^3.1.4",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@vue/compiler-dom": "^3.5.13",
    "@vue/server-renderer": "^3.5.13",
    "@vue/test-utils": "^2.4.6",
    "babel-jest": "^29.7.0",
    "check-es-version": "^1.5.1",
    "core-js": "^3.40.0",
    "cross-env": "^7.0.3",
    "email-validator": "^2.0.4",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-vue": "^9.32.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-jsdom-global": "^4.0.0",
    "jest-extended": "^4.0.2",
    "jsdoc": "^4.0.4",
    "rollup": "^4.34.6",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "vue": "^3.5.13"
  },
  "packageManager": "yarn@4.5.0"
}
